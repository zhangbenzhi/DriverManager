<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drivermanager">

    <application
        android:name="com.example.drivermanager.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.drivermanager.activity.SystemActivity" />
        <activity android:name="com.example.drivermanager.activity.UserManagerActivity" />
        <activity android:name="com.example.drivermanager.activity.SelfServiceDataActivity" />
        <activity android:name="com.example.drivermanager.activity.RegisterActivity" />
        <activity android:name="com.example.drivermanager.activity.LoginActivity" />
        <activity android:name="com.example.drivermanager.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.drivermanager.activity.MainActivity" />
        <activity android:name=".activity.AllServiceDataActivity" />
        <activity android:name=".activity.ServiceDataDetailActivity" />
    </application>

</manifest>